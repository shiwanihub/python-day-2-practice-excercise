   "source": [
    "\n",
    "total_sum = 0\n",
    "\n",
    "for number in range(1, 500):\n",
    "    # Check if the number is a multiple of 3 or 5\n",
    "    if number % 3 == 0 or number % 5 == 0:\n",
    "        total_sum += number\n",
    "\n",
    "print(\"Sum of multiples of 3 or 5 below 500:\", total_sum)\n"
   ]
  }
